{
  "name": "md2conf",
  "description": "Markdown to Confluence markup.",
  "version": "1.7.1",
  "author": {
    "email": "zac@zacanger.com",
    "name": "Zac Anger",
    "url": "http://zacanger.com"
  },
  "license": "MIT",
  "main": "index.js",
  "files": [
    "cli.js",
    "index.js"
  ],
  "bin": "./cli.js",
  "scripts": {
    "preversion": "npm t && sortpack",
    "test": "run-s -s test:lint test:cover",
    "test:cover": "nyc npm run test:tape",
    "test:lint": "eslint -c .eslintrc.json *.js",
    "test:tape": "tape test.js"
  },
  "homepage": "https://github.com/zacanger/md2conf",
  "repository": {
    "type": "git",
    "url": "https://github.com/zacanger/md2conf.git"
  },
  "bugs": {
    "url": "https://github.com/zacanger/md2conf/issues"
  },
  "keywords": [
    "confluence",
    "convert",
    "markdown",
    "markup",
    "md"
  ],
  "dependencies": {
    "get-stdin": "7.0.0",
    "marked": "0.6.2"
  },
  "devDependencies": {
    "babel-eslint": "10.0.1",
    "eslint": "5.16.0",
    "eslint-config-zacanger": "3.4.2",
    "eslint-plugin-babel": "5.3.0",
    "eslint-plugin-import": "2.17.2",
    "eslint-plugin-node": "8.0.1",
    "eslint-plugin-promise": "4.1.1",
    "eslint-plugin-unicorn": "8.0.2",
    "npm-run-all": "4.1.5",
    "nyc": "14.0.0",
    "sortpack": "1.1.4",
    "tape": "4.10.1"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "nyc": {
    "include": [
      "index.js"
    ],
    "instrument": true,
    "reporter": [
      "text",
      "html",
      "json"
    ],
    "sourceMap": true
  }
}
