{
  "name": "md2conf",
  "description": "Markdown to Confluence markup.",
  "version": "2.0.1",
  "author": {
    "email": "zac@zacanger.com",
    "name": "Zac Anger",
    "url": "http://zacanger.com"
  },
  "license": "MIT",
  "main": "index.js",
  "files": [
    "cli.js",
    "index.js"
  ],
  "bin": "./cli.js",
  "scripts": {
    "preversion": "npm t && sortpack",
    "test": "run-s -s test:lint test:cover",
    "test:cover": "nyc npm run test:tape",
    "test:lint": "standard",
    "test:tape": "tape test.js"
  },
  "homepage": "https://github.com/zacanger/md2conf",
  "repository": {
    "type": "git",
    "url": "https://github.com/zacanger/md2conf.git"
  },
  "bugs": {
    "url": "https://github.com/zacanger/md2conf/issues"
  },
  "keywords": [
    "confluence",
    "convert",
    "markdown",
    "markup",
    "md"
  ],
  "dependencies": {
    "get-stdin": "8.0.0",
    "marked": "4.0.17"
  },
  "devDependencies": {
    "npm-run-all": "4.1.5",
    "nyc": "15.1.0",
    "sortpack": "2.3.0",
    "standard": "17.0.0",
    "tape": "5.5.3"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "nyc": {
    "include": [
      "index.js"
    ],
    "instrument": true,
    "reporter": [
      "text",
      "html",
      "json"
    ],
    "sourceMap": true
  }
}
